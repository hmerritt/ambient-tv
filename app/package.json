{
    "name": "ambient-tv",
    "version": "1.6.12",
    "main": "expo-router/entry",
    "scripts": {
        "start": "expo start --tunnel",
        "android": "expo start --android --tunnel",
        "android:build": "expo export --platform android && eas build --platform android --profile production --local",
        "android:build:development": "expo export --platform android && eas build --platform android --profile development --local --clear-cache",
        "android:build:remote": "eas build --platform android --profile production",
        "android:build:development:remote": "eas build --platform android --profile development",
        "ios": "expo start --ios",
        "ios:build:remote": "eas build --platform ios --profile production",
        "ios:build:development:remote": "eas build --platform ios --profile development",
        "web": "expo start --web",
        "web:build": "expo export --platform web",
        "update": "yarn upgrade-interactive"
    },
    "dependencies": {
        "@expo/metro-runtime": "4.0.0",
        "@expo/vector-icons": "14.0.4",
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-native-masked-view/masked-view": "0.3.1",
        "@react-navigation/native": "7.0.0",
        "@reduxjs/toolkit": "2.3.0",
        "axios": "1.7.7",
        "chance": "1.1.12",
        "expo": "52.0.7",
        "expo-av": "15.0.1",
        "expo-build-properties": "0.13.1",
        "expo-constants": "17.0.3",
        "expo-dev-client": "5.0.2",
        "expo-font": "13.0.1",
        "expo-keep-awake": "14.0.1",
        "expo-linear-gradient": "14.0.1",
        "expo-linking": "7.0.2",
        "expo-location": "18.0.1",
        "expo-navigation-bar": "4.0.3",
        "expo-router": "4.0.6",
        "expo-splash-screen": "0.29.11",
        "expo-status-bar": "2.0.0",
        "moment": "2.30.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.2",
        "react-native-gesture-handler": "2.20.2",
        "react-native-reanimated": "3.16.1",
        "react-native-rss-parser": "1.5.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "4.0.0",
        "react-native-web": "0.19.13",
        "react-native-webview": "13.12.2",
        "react-redux": "9.1.2",
        "redux": "5.0.1",
        "redux-logger": "3.0.6"
    },
    "devDependencies": {
        "@babel/core": "7.26.0",
        "@babel/plugin-transform-private-methods": "7.25.9",
        "@babel/plugin-transform-private-property-in-object": "7.25.9",
        "@trivago/prettier-plugin-sort-imports": "4.3.0",
        "@types/react": "18.3.12",
        "expo-module-scripts": "^4.0.2",
        "prettier": "3.3.3",
        "typescript": "5.6.3"
    },
    "private": true,
    "packageManager": "yarn@4.5.1"
}
